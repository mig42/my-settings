{
    "editor.renderWhitespace": "all",
    "editor.autoIndent": "advanced",
    "editor.rulers": [ 80, 100 ],
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "vue"
    ],
    "window.zoomLevel": 0,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/.plastic": true,
        "**/.bplastic": true,
        "publish": true,
        "node_modules": true,
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "breadcrumbs.enabled": true,
    "window.titleBarStyle": "custom",
    "editor.fontFamily": "'Fira Code', Consolas, 'Courier New', monospace",
    "editor.fontLigatures": true,
    "editor.suggestSelection": "first",
    "xml.java.home": "C:\\Program Files\\Java\\jdk1.8.0_212",
    "workbench.colorTheme": "Material Theme Darker High Contrast",
    "explorer.confirmDragAndDrop": false,
    "git.autofetch": true,
    "terminal.external.windowsExec": "C:\\Program Files\\PowerShell\\7\\pwsh.exe",
    "terminal.integrated.allowChords": false,
    "terminal.integrated.shell.windows": "C:\\Program Files\\PowerShell\\7\\pwsh.exe",
    "terminal.integrated.shellArgs.windows": ["-NoLogo"],
    "editor.tabSize": 2,
    "[csharp]": {
        "editor.tabSize": 4
    },
    "[razor]": {
        "editor.rulers": [ 100, 120 ]
    },
    "[aspnetcorerazor]": {
        "editor.rulers": [ 100, 120 ]
    },
    "[html]": {
        "editor.rulers": [ 100, 120 ]
    },
    "[vue-html]": {
        "editor.rulers": [ 80, 100, 120 ]
    },
    "[markdown]": {
        "editor.wordWrap": "on",
        "editor.quickSuggestions": false,
        "files.trimTrailingWhitespace": false
    },
    "mochaExplorer.files": "out/test/**/*.js",
    "mochaExplorer.require": [
        "vscode-test"
    ],
    "diffEditor.renderSideBySide": false,
    "workbench.iconTheme": "material-icon-theme",
    "files.associations": {
        "*.include": "xml"
    },
    "files.trimTrailingWhitespace": true,
    "powershell.powerShellDefaultVersion": "PowerShell (x64)",
    "vsonline.authentication.accountProvider": "Microsoft",
    "editor.fontSize": 12,
    "vsonline.accountProvider": "Microsoft",
    "codespaces.accountProvider": "Microsoft",
    "java.semanticHighlighting.enabled": true,
    "git.alwaysSignOff": true,
    "githubIssues.queries": [

        {
            "label": "My Issues",
            "query": "default"
        },
        {
            "label": "Created Issues",
            "query": "author:${user} state:open repo:${owner}/${repository} sort:created-desc"
        },
        {
            "label": "Open Issues",
            "query": "state:open repo:${owner}/${repository} sort:created-desc"
        }
    ],
    "git.confirmSync": false
}