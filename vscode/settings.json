{
    "workbench.iconTheme": "material-icon-theme",
    "workbench.colorTheme": "PowerShell ISE",
    "editor.renderWhitespace": "all",
    "editor.autoIndent": "advanced",
    "editor.rulers": [ 80, 100 ],
    "eslint.run": "onType",
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "vue",
        "html"
    ],
    "editor.codeActionsOnSave": {
        "source.fixAll": true
    },
    "vetur.format.defaultFormatter.html": "js-beautify-html",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/.plastic": true,
        "**/.bplastic": true,
        "publish": true,
        "node_modules": true,
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "breadcrumbs.enabled": true,
    "window.titleBarStyle": "custom",
    "editor.fontFamily": "'Fira Code', Consolas, 'Courier New', monospace",
    "editor.fontLigatures": true,
    "editor.suggestSelection": "first",
    "xml.java.home": "C:\\Program Files\\Java\\jdk1.8.0_212",
    "explorer.confirmDragAndDrop": false,
    "git.autofetch": true,
    "terminal.external.windowsExec": "C:\\Program Files\\PowerShell\\7\\pwsh.exe",
    "terminal.integrated.allowChords": false,
    "terminal.integrated.shell.windows": "C:\\Program Files\\PowerShell\\7\\pwsh.exe",
    "terminal.integrated.shellArgs.windows": [
        "-NoLogo"
    ],
    "editor.tabSize": 2,
    "[csharp]": {
        "editor.tabSize": 4
    },
    "[razor]": {
        "editor.rulers": [ 100, 120 ]
    },
    "[aspnetcorerazor]": {
        "editor.rulers": [ 100, 120 ]
    },
    "[html]": {
        "editor.rulers": [ 100, 120 ]
    },
    "[vue-html]": {
        "editor.rulers": [ 80, 100, 120 ]
    },
    "[markdown]": {
        "editor.wordWrap": "on",
        "editor.quickSuggestions": false,
        "files.trimTrailingWhitespace": false,
        "editor.rulers": [
            100,
            120
        ]
    },
    "mochaExplorer.files": "out/test/**/*.js",
    "mochaExplorer.require": [
        "vscode-test"
    ],
    "diffEditor.renderSideBySide": false,
    "files.associations": {
        "*.include": "xml"
    },
    "python.jediEnabled": false,
    "vsonline.authentication.accountProvider": "Microsoft",
    "vsonline.accountProvider": "Microsoft",
    "codespaces.accountProvider": "Microsoft",
    "java.semanticHighlighting.enabled": true,
    "files.trimTrailingWhitespace": true,
    "powershell.powerShellDefaultVersion": "PowerShell (x64)",
    "editor.fontSize": 12,
    "githubIssues.queries": [
        {
            "label": "My Issues",
            "query": "default"
        },
        {
            "label": "Created Issues",
            "query": "author:${user} state:open repo:${owner}/${repository} sort:created-desc"
        },
        {
            "label": "Open Issues",
            "query": "state:open repo:${owner}/${repository} sort:created-desc"
        }
    ],
    "git.confirmSync": false,
    "python.languageServer": "Pylance",
    "python.condaPath": "C:\\Users\\miguel\\anaconda3\\Scripts\\conda.exe",
    "vim.visualModeKeyBindings": [
        {
            "before": [
                "<leader>",
                "c"
            ],
            "commands": [
                "editor.action.commentLine"
            ]
        }
    ],
    "editor.linkedEditing": true,
    "[javascript]": {
        "editor.defaultFormatter": "HookyQR.beautify"
    },
    "vetur.validation.templateProps": true,
    "vetur.useWorkspaceDependencies": true,
    "FSharp.useSdkScripts": true,
    "vim.handleKeys": {
        "<C-d>": true,
        "<C-n>": false,
        "<C-w>": false
    },
    "[xml]": {
        "editor.defaultFormatter": "redhat.vscode-xml"
    },
    "git.alwaysSignOff": true,
    "mssql.connections": [
        {
            "server": "{{put-server-name-here}}",
            "database": "{{put-database-name-here}}",
            "user": "{{put-username-here}}",
            "password": "{{put-password-here}}"
        }
    ],
    "workbench.colorCustomizations": {
        "[Material Theme Darker High Contrast]": {
            "activityBarBadge.background": "#64FFDA",
            "activityBar.activeBorder": "#64FFDA",
            "list.activeSelectionForeground": "#64FFDA",
            "list.inactiveSelectionForeground": "#64FFDA",
            "list.highlightForeground": "#64FFDA",
            "scrollbarSlider.activeBackground": "#64FFDA50",
            "editorSuggestWidget.highlightForeground": "#64FFDA",
            "textLink.foreground": "#64FFDA",
            "progressBar.background": "#64FFDA",
            "pickerGroup.foreground": "#64FFDA",
            "tab.activeBorder": "#64FFDA",
            "notificationLink.foreground": "#64FFDA",
            "editorWidget.resizeBorder": "#64FFDA",
            "editorWidget.border": "#64FFDA",
            "settings.modifiedItemIndicator": "#64FFDA",
            "settings.headerForeground": "#64FFDA",
            "panelTitle.activeBorder": "#64FFDA",
            "breadcrumb.activeSelectionForeground": "#64FFDA",
            "menu.selectionForeground": "#64FFDA",
            "menubar.selectionForeground": "#64FFDA",
            "editor.findMatchBorder": "#64FFDA",
            "selection.background": "#64FFDA40",
            "statusBarItem.remoteBackground": "#64FFDA"
        }
    },
    "materialTheme.accent": "Bright Teal"
}